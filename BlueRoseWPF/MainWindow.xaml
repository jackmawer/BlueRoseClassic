<Window x:Class="BlueRoseLauncher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BlueRoseLauncher"
        mc:Ignorable="d"
        Title="Blue Rose" Height="583.702" Width="796.268"
        Background="#3263a8"
        ResizeMode="CanMinimize">
    <Grid>
        <Button x:Name="Start" HorizontalAlignment="Left" Margin="10,511,0,0" VerticalAlignment="Top" Width="75" Height="20" Content="Play" Click="Start_Click">
        </Button>
        <WebBrowser x:Name="WebMain" HorizontalAlignment="Left" Height="496" Margin="10,10,0,0" VerticalAlignment="Top" Width="770"/>
        <Button x:Name="FSONEws" Content="FreeSO News" HorizontalAlignment="Left" Margin="682,511,0,0" VerticalAlignment="Top" Width="98" Click="FSONews_Click"/>
        <Button x:Name="LauncherNews" Content="Blue Rose News" HorizontalAlignment="Left" Margin="572,511,0,0" VerticalAlignment="Top" Width="105" Click="LauncherNews_Click"/>
        <Button x:Name="StartIDE" Content="Develop" HorizontalAlignment="Left" Margin="90,511,0,0" VerticalAlignment="Top" Width="75" Click="StartIDE_Click"/>

    </Grid>
</Window>